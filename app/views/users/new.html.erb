<h2>Sign up</h2>

<%= form_for(@user, url: signup_path) do |f| %>


<%= f.label :name %><br />
<%= f.text_field :name%>

<%= f.label :password_digest %><br />
<%= f.password_field :password_digest %>

<%= f.label :password_confirmation %><br />
<%= f.password_field :password_confirmation%>

<%= f.submit "Sign up" %>

<% end %>